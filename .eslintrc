{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "react/jsx-uses-vars": 1,
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "camelcase": 0,
    "space-before-function-paren": 0,
    "semi": 0,
    "no-unused-vars": 1,
    "keyword-spacing": 0
  },
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "jest": true
  },
}
